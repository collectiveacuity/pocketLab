{
  "schema": {
    "verbose": false,
    "logging": false
  },
  "components": {
    ".logging": {
      "default_value": true,
      "field_description": "Toggle to enable/disable lab logging",
      "field_metadata": {
        "cli_flags": [ "-z", "--zzz" ],
        "cli_help": "turn off lab logging (logging helps debugging)"
      }
    },
    ".verbose": {
      "default_value": true,
      "field_description": "Toggle to enable/disable lab bot messages",
      "field_metadata": {
        "cli_flags": [ "-q", "--quiet" ],
        "cli_help": "turn off lab process messages"
      }
    }
  }
}